{
    "name": "rsalas/cwh",
    "homepage": "https://github.com/rsalas/cwh",
    "type": "library",
    "description": "AWS CloudWatch Handler for Monolog library",
    "keywords": [
        "aws",
        "cloudwatch",
        "monolog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rodolfo Salas",
            "email": "rsalas@usalinux.org"
        }
    ],
    "support": {
        "issues": "https://github.com/rsalas/cwh/issues",
        "source": "https://github.com/rsalas/cwh"
    },
    "require": {
        "php": "^8",
        "monolog/monolog": "^3.0",
        "aws/aws-sdk-php": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Rsalas\\Cwh\\": "src"
        }
    },
    "minimum-stability": "stable"
}
